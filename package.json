{
  "name": "react-template",
  "version": "0.0.1",
  "description": "react template for argo projects",
  "main": "index.js",
  "packageManager": "pnpm@7.25.0",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "prepare": "is-ci || husky install",
    "coverage": "node ./coverage-server.js",
    "lint": "eslint \"src/**/*.{js*,ts}\" --fix",
    "pretty": "prettier --write \"src/**/*.{js*,ts}\""
  },
  "author": "Ryan Brogden",
  "license": "ISC",
  "devDependencies": {
    "@argodevops/eslint-config": "^1.2.0",
    "@argodevops/semantic-release-teams": "^1.0.11",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@codedependant/semantic-release-docker": "^4.3.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@webpack-cli/serve": "^2.0.1",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.32.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.3",
    "semantic-release": "^20.0.2",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.1.1"
  }
}
